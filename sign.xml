<project name="DicomBrowser" default="sign">
	<property name="project.name" value="DicomBrowser" />
	<property name="VERSION" value="1.3" />
	
	<property name="target" value="target" />
	<mkdir dir="${target}" />

	<target name="sign">
		<signjar jar="${target}/${project.name}-${VERSION}-jar-with-dependencies.jar" alias="####"
			storepass="####" keypass="####" />
	</target>
</project>
